    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:simplepdl="http://simplepdl"
    xsi:schemaLocation="http://simplepdl SimplePDL.ecore"
    name="TestFonctionnel">
  <processElements
      xsi:type="simplepdl:WorkDefinition"
      linksToPredecessors="//@processElements.6"
      linksToSuccessors="//@processElements.3"
      name="Activite1">
    <askedRessource
        ressource="//@processElements.1"
        quantity="5"/>
  </processElements>
  <processElements
      xsi:type="simplepdl:Ressource"
      name="Ressource1"
      stock="3"
      work="//@processElements.0/@askedRessource.0"/>
  <processElements
      xsi:type="simplepdl:WorkDefinition"
      linksToPredecessors="//@processElements.3"
      linksToSuccessors="//@processElements.5"
      name="Activite22">
    <askedRessource
        ressource="//@processElements.8"
        quantity="18"/>
  </processElements>
  <processElements
      xsi:type="simplepdl:WorkSequence"
      predecessor="//@processElements.0"
      successor="//@processElements.2"/>
  <processElements
      xsi:type="simplepdl:WorkDefinition"
      linksToPredecessors="//@processElements.5"
      linksToSuccessors="//@processElements.6"
      name="Activite333"/>
  <processElements
      xsi:type="simplepdl:WorkSequence"
      predecessor="//@processElements.2"
      successor="//@processElements.4"/>
  <processElements
      xsi:type="simplepdl:WorkSequence"
      predecessor="//@processElements.4"
      successor="//@processElements.0"/>
  <processElements
      xsi:type="simplepdl:Guidance"
      elements="//@processElements.4 //@processElements.2"
      text="Le nom de l'activité dépend de son ordre de création : l'activité x se nommera x*string(x) (donc 22, 333, 4444, etc)."/>
  <processElements
      xsi:type="simplepdl:Ressource"
      name="Ordinateur"
      stock="10"
      work="//@processElements.2/@askedRessource.0"/>
